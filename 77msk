            </form>
            
            <div class="mt-4 text-xs text-gray-500 text-center">
                üí° –ù–∞—Å—Ç—Ä–æ–π—Ç–µ —Å–≤–æ–µ–≥–æ –±–æ—Ç–∞: –∑–∞–º–µ–Ω–∏—Ç–µ —Ç–æ–∫–µ–Ω –∏ Chat ID –≤ –∫–æ–¥–µ
            </div>
        </div>
    </div>

    <script>
        // ========================================
        // –ù–ê–°–¢–†–û–ô–ö–ò TELEGRAM –ë–û–¢–ê
        // ========================================
        // –ó–∞–º–µ–Ω–∏—Ç–µ —ç—Ç–∏ –∑–Ω–∞—á–µ–Ω–∏—è –Ω–∞ –≤–∞—à–∏ –Ω–∞—Å—Ç–æ—è—â–∏–µ –¥–∞–Ω–Ω—ã–µ:
        
        const TELEGRAM_CONFIG = {
            // –¢–æ–∫–µ–Ω –≤–∞—à–µ–≥–æ –±–æ—Ç–∞ (–ø–æ–ª—É—á–∏—Ç–µ —É @BotFather)
            BOT_TOKEN: '8011980650:AAHnLwWY42QP7cjDqo_y-QGYEcke78xBLmA', // –ù–∞–ø—Ä–∏–º–µ—Ä: '123456789:ABCdefGHIjklMNOpqr'
            
            // –í–∞—à Chat ID (–ø–æ–ª—É—á–∏—Ç–µ —É @userinfobot)
            CHAT_ID: '7492262966', // –ù–∞–ø—Ä–∏–º–µ—Ä: '123456789'
            
            // –í–∫–ª—é—á–∏—Ç—å/–≤—ã–∫–ª—é—á–∏—Ç—å –æ—Ç–ø—Ä–∞–≤–∫—É –≤ Telegram (true/false)
            ENABLED: true
        };
        
        // ========================================
        // –ú–û–ë–ò–õ–¨–ù–û–ï –ú–ï–ù–Æ
        // ========================================
        
        function toggleMobileMenu() {
            const mobileMenu = document.getElementById('mobileMenu');
            const hamburger = document.querySelector('.hamburger');
            
            mobileMenu.classList.toggle('active');
            hamburger.classList.toggle('active');
            
            // –ë–ª–æ–∫–∏—Ä—É–µ–º —Å–∫—Ä–æ–ª–ª –∫–æ–≥–¥–∞ –º–µ–Ω—é –æ—Ç–∫—Ä—ã—Ç–æ
            if (mobileMenu.classList.contains('active')) {
                document.body.style.overflow = 'hidden';
            } else {
                document.body.style.overflow = '';
            }
        }
        
        function closeMobileMenu() {
            const mobileMenu = document.getElementById('mobileMenu');
            const hamburger = document.querySelector('.hamburger');
            
            mobileMenu.classList.remove('active');
            hamburger.classList.remove('active');
            document.body.style.overflow = '';
        }
        
        // ========================================
        // –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò –°–ê–ô–¢–ê
        // ========================================
        
        // Smooth scrolling
        function scrollToSection(sectionId) {
            document.getElementById(sectionId).scrollIntoView({ behavior: 'smooth' });
        }

        // Fade in animation on scroll
        function handleScrollAnimations() {
            const elements = document.querySelectorAll('.fade-in');
            elements.forEach(element => {
                const elementTop = element.getBoundingClientRect().top;
                const elementVisible = 150;
                
                if (elementTop < window.innerHeight - elementVisible) {
                    element.classList.add('visible');
                }
            });
        }

        // Contact modal functions
        function openContactModal() {
            document.getElementById('contactNumbersModal').classList.remove('hidden');
            closeMobileMenu(); // –ó–∞–∫—Ä—ã–≤–∞–µ–º –º–æ–±–∏–ª—å–Ω–æ–µ –º–µ–Ω—é –µ—Å–ª–∏ –æ—Ç–∫—Ä—ã—Ç–æ
        }

        function closeContactModal() {
            document.getElementById('contactNumbersModal').classList.add('hidden');
        }

        // Developer contact functions
        function openDeveloperContact() {
            document.getElementById('developerContactModal').classList.remove('hidden');
        }

        function closeDeveloperContact() {
            document.getElementById('developerContactModal').classList.add('hidden');
        }

        // Contact form functions
        function openContactForm() {
            document.getElementById('contactFormModal').classList.remove('hidden');
            closeMobileMenu(); // –ó–∞–∫—Ä—ã–≤–∞–µ–º –º–æ–±–∏–ª—å–Ω–æ–µ –º–µ–Ω—é –µ—Å–ª–∏ –æ—Ç–∫—Ä—ã—Ç–æ
        }

        function closeContactForm() {
            document.getElementById('contactFormModal').classList.add('hidden');
        }

        // ========================================
        // TELEGRAM –ò–ù–¢–ï–ì–†–ê–¶–ò–Ø
        // ========================================
        
        async function submitForm(event) {
            event.preventDefault();
            
            const form = event.target;
            const formData = new FormData(form);
            const loading = form.querySelector('.loading');
            const submitText = form.querySelector('.submit-text');
            const submitButton = form.querySelector('button[type="submit"]');
            
            // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –∑–∞–≥—Ä—É–∑–∫—É
            loading.style.display = 'inline-block';
            submitText.textContent = '–û—Ç–ø—Ä–∞–≤–ª—è–µ–º...';
            submitButton.disabled = true;
            
            // –°–æ–±–∏—Ä–∞–µ–º –¥–∞–Ω–Ω—ã–µ —Ñ–æ—Ä–º—ã
            const data = {
                name: formData.get('name'),
                email: formData.get('email'),
                phone: formData.get('phone'),
                message: formData.get('message') || '–ù–µ —É–∫–∞–∑–∞–Ω–æ'
            };
            
            // –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ Telegram
            if (!TELEGRAM_CONFIG.ENABLED || 
                TELEGRAM_CONFIG.BOT_TOKEN === 'YOUR_BOT_TOKEN_HERE' || 
                TELEGRAM_CONFIG.CHAT_ID === 'YOUR_CHAT_ID_HERE') {
                
                // –ï—Å–ª–∏ Telegram –Ω–µ –Ω–∞—Å—Ç—Ä–æ–µ–Ω, –ø–æ–∫–∞–∑—ã–≤–∞–µ–º –¥–µ–º–æ
                setTimeout(() => {
                    loading.style.display = 'none';
                    submitText.textContent = '–û—Ç–ø—Ä–∞–≤–∏—Ç—å –≤ Telegram';
                    submitButton.disabled = false;
                    
                    alert(`üéâ –î–µ–º–æ –æ—Ç–ø—Ä–∞–≤–∫–∏ –≤ Telegram!

üìù –î–∞–Ω–Ω—ã–µ –∑–∞—è–≤–∫–∏:
üë§ –ò–º—è: ${data.name}
üìß Email: ${data.email}
üì± –¢–µ–ª–µ—Ñ–æ–Ω: ${data.phone}
üí¨ –°–æ–æ–±—â–µ–Ω–∏–µ: ${data.message}

‚ö†Ô∏è –î–ª—è —Ä–µ–∞–ª—å–Ω–æ–π –æ—Ç–ø—Ä–∞–≤–∫–∏ –Ω–∞—Å—Ç—Ä–æ–π—Ç–µ —Ç–æ–∫–µ–Ω –±–æ—Ç–∞ –∏ Chat ID –≤ –∫–æ–¥–µ.`);
                    
                    closeContactForm();
                    form.reset();
                }, 1000);
                
                return;
            }
            
            // –§–æ—Ä–º–∏—Ä—É–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ –¥–ª—è Telegram
            const telegramMessage = `üöÄ *–ù–æ–≤–∞—è –∑–∞—è–≤–∫–∞ —Å —Å–∞–π—Ç–∞ 77MSK*

üë§ *–ò–º—è:* ${data.name}
üìß *Email:* ${data.email}
üì± *–¢–µ–ª–µ—Ñ–æ–Ω:* ${data.phone}
üí¨ *–°–æ–æ–±—â–µ–Ω–∏–µ:* ${data.message}

üìÖ *–î–∞—Ç–∞:* ${new Date().toLocaleDateString('ru-RU')}
üïê *–í—Ä–µ–º—è:* ${new Date().toLocaleTimeString('ru-RU')}

#–∑–∞—è–≤–∫–∞ #–Ω–æ–≤—ã–π–∫–ª–∏–µ–Ω—Ç`;

            // –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –≤ Telegram
            try {
                const telegramURL = `https://api.telegram.org/bot${TELEGRAM_CONFIG.BOT_TOKEN}/sendMessage`;
                
                const response = await fetch(telegramURL, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                        chat_id: TELEGRAM_CONFIG.CHAT_ID,
                        text: telegramMessage,
                        parse_mode: 'Markdown'
                    })
                });
                
                const result = await response.json();
                
                // –í–æ–∑–≤—Ä–∞—â–∞–µ–º –∫–Ω–æ–ø–∫—É –≤ –∏—Å—Ö–æ–¥–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ
                loading.style.display = 'none';
                submitText.textContent = '–û—Ç–ø—Ä–∞–≤–∏—Ç—å –≤ Telegram';
                submitButton.disabled = false;
                
                if (response.ok && result.ok) {
                    // –£—Å–ø–µ—à–Ω–∞—è –æ—Ç–ø—Ä–∞–≤–∫–∞
                    alert('‚úÖ –ó–∞—è–≤–∫–∞ —É—Å–ø–µ—à–Ω–æ –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–∞ –≤ Telegram!\n\n–ú—ã —Å–≤—è–∂–µ–º—Å—è —Å –≤–∞–º–∏ –≤ –±–ª–∏–∂–∞–π—à–µ–µ –≤—Ä–µ–º—è.');
                    closeContactForm();
                    form.reset();
                } else {
                    // –û—à–∏–±–∫–∞ Telegram API
                    console.error('Telegram Error:', result);
                    alert('‚ùå –û—à–∏–±–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏ –≤ Telegram.\n\n–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ –ø–æ–∑–∂–µ –∏–ª–∏ —Å–≤—è–∂–∏—Ç–µ—Å—å —Å –Ω–∞–º–∏ –ø–æ —Ç–µ–ª–µ—Ñ–æ–Ω—É.');
                }
                
            } catch (error) {
                // –û—à–∏–±–∫–∞ —Å–µ—Ç–∏
                console.error('Network Error:', error);
                loading.style.display = 'none';
                submitText.textContent = '–û—Ç–ø—Ä–∞–≤–∏—Ç—å –≤ Telegram';
                submitButton.disabled = false;
                
                alert('‚ùå –û—à–∏–±–∫–∞ —Å–µ—Ç–∏.\n\n–ü—Ä–æ–≤–µ—Ä—å—Ç–µ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –∫ –∏–Ω—Ç–µ—Ä–Ω–µ—Ç—É –∏ –ø–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.');
            }
        }

        // Navbar scroll effect
        function handleNavbarScroll() {
            const navbar = document.querySelector('.navbar');
            if (window.scrollY > 100) {
                navbar.style.background = 'rgba(0, 0, 0, 0.95)';
            } else {
                navbar.style.background = 'rgba(0, 0, 0, 0.9)';
            }
        }

        // ========================================
        // EVENT LISTENERS
        // ========================================
        
        // –ê–Ω–∏–º–∞—Ü–∏–∏ –∏ –Ω–∞–≤–∏–≥–∞—Ü–∏—è
        window.addEventListener('scroll', handleScrollAnimations);
        window.addEventListener('scroll', handleNavbarScroll);
        window.addEventListener('load', handleScrollAnimations);

        // Smooth anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({ behavior: 'smooth', block: 'start' });
                    closeMobileMenu(); // –ó–∞–∫—Ä—ã–≤–∞–µ–º –º–æ–±–∏–ª—å–Ω–æ–µ –º–µ–Ω—é –ø–æ—Å–ª–µ –∫–ª–∏–∫–∞ –ø–æ —Å—Å—ã–ª–∫–µ
                }
            });
        });

        // Close modal on outside click
        document.getElementById('contactNumbersModal').addEventListener('click', function(e) {
            if (e.target === this) {
                closeContactModal();
            }
        });

        document.getElementById('developerContactModal').addEventListener('click', function(e) {
            if (e.target === this) {
                closeDeveloperContact();
            }
        });

        document.getElementById('contactFormModal').addEventListener('click', function(e) {
            if (e.target === this) {
                closeContactForm();
            }
        });

        // Close mobile menu on window resize
        window.addEventListener('resize', function() {
            if (window.innerWidth > 768) {
                closeMobileMenu();
            }
        });
        
        // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Å—Ç–∞—Ç—É—Å –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ
        window.addEventListener('load', function() {
            if (TELEGRAM_CONFIG.BOT_TOKEN === 'YOUR_BOT_TOKEN_HERE' || 
                TELEGRAM_CONFIG.CHAT_ID === 'YOUR_CHAT_ID_HERE') {
                console.log('ü§ñ Telegram –±–æ—Ç –Ω–µ –Ω–∞—Å—Ç—Ä–æ–µ–Ω. –†–∞–±–æ—Ç–∞–µ—Ç –≤ –¥–µ–º–æ-—Ä–µ–∂–∏–º–µ.');
                console.log('üìù –ò–Ω—Å—Ç—Ä—É–∫—Ü–∏—è –ø–æ –Ω–∞—Å—Ç—Ä–æ–π–∫–µ:');
                console.log('1. –°–æ–∑–¥–∞–π—Ç–µ –±–æ—Ç–∞ —á–µ—Ä–µ–∑ @BotFather');
                console.log('2. –ü–æ–ª—É—á–∏—Ç–µ —Ç–æ–∫–µ–Ω –∏ –∑–∞–º–µ–Ω–∏—Ç–µ YOUR_BOT_TOKEN_HERE');
                console.log('3. –ü–æ–ª—É—á–∏—Ç–µ Chat ID —á–µ—Ä–µ–∑ @userinfobot');
                console.log('4. –ó–∞–º–µ–Ω–∏—Ç–µ YOUR_CHAT_ID_HERE –Ω–∞ –≤–∞—à Chat ID');
            } else {
                console.log('‚úÖ Telegram –±–æ—Ç –Ω–∞—Å—Ç—Ä–æ–µ–Ω –∏ –≥–æ—Ç–æ–≤ –∫ —Ä–∞–±–æ—Ç–µ!');
            }
        });
    </script>
</body>
</html>
